{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": ["single"],
        "no-console": [false],
        "no-unused-expression": [false],
        "no-var-requires": false,
        "trailing-comma": [false],
        "variable-name": ["allow-leading-underscore"],
        "interface-name": ["never-prefix"]
    },
    "rulesDirectory": []
}
